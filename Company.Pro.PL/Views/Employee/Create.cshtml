@model CreateEmployeeDto
@{
    ViewData["Title"] = "Create";
}

<h1>Create Employee </h1>

<br />
<br />

<form asp-action="Create" asp-controller="Employee" method="post" enctype="multipart/form-data">
    <partial name="Create-Edit-PartialView/_CreateEditPartialView" />
    <br />
    <button type="submit" class="btn btn-primary">Create</button>
    <a asp-action="Index" asp-controller="Employee" class="btn btn-secondary">Back to List</a>
</form>
@section ValidationSection{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}